#include<stdio.h>
#include<stdlib.h>
struct node{
	int data;
   struct node*next;	
}*top,*n;
void push(int num){
	int size=0;
	n=(struct node*)malloc(sizeof(struct node));
	if(!n){
		printf("overflow");
		exit(1);	
	}
	else{
		n->data=num;
		n->next=top;
		top=n;
	}
	printf("\n");
}
void pop(){
	struct node*t;
	
	if(top==NULL){
		printf("underflow");
		exit(1);
	}
	else{
		t=top;
		top=top->next;
		t->next=NULL;
		free(t);
	}
	printf("\n");
}
void peek(){
	if(top==NULL){
		printf("underflow");
		exit(1);
	}
	else{
		printf("%d",top->data);
	}
	printf("\n");
}
void display(){

struct node*t;
if(top==NULL){
	printf("underflow");
	exit(1);
}
else{
	t=top;
	while(t!=NULL){
		printf("%d",t->data);
		t=t->next;
	}
}
}
int main(){
   push(30);
   push(20);
   push(10);
   display();
   pop();
   display();
   peek();
   return 0; 
}

