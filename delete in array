#include <stdio.h>
int main() {
  int n=5;
int arr[n]={1,2,3,4,5};
  int position=3;
  printf("enter the elements");
for (int i=0;i<n;i++) {
    scanf("%d",&arr[i]);
}
    for (int i=position;i<n;i++) {
        arr[i]=arr[i+1];
      n--;
    }
    for (int i=0;i<n;i++) {
    printf("%d",arr[i]);
    }
    printf("\nm.shashanka");
    printf("\n192311083");
    printf("\n");
return 0;
}
